class Node:
    def __init__(self,data):
        self.data=data
        self.next=None

def create_linked_list(values):
    if not values:
        return None

    head=Node(values[0])      # create first node
    current=head              # pointer to move through list

    for v in values[1:]:      # loop through remaining values
        current.next=Node(v)  # link new node
        current=current.next  # move pointer

    return head

def print_list(head):
    temp=head
    while temp:
        print(temp.data,end=" ")
        temp=temp.next

nums=list(map(int,input("Enter values: ").split()))
head=create_linked_list(nums)
print_list(head)
